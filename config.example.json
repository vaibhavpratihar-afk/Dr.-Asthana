{
  "jira": {
    "baseUrl": "https://your-org.atlassian.net",
    "email": "you@example.com",
    "apiToken": "YOUR_JIRA_API_TOKEN",
    "label": "your-trigger-label",
    "labelProcessed": "your-done-label",
    "maxComments": 100,
    "fields": {
      "affectedSystems": "customfield_10056",
      "fixVersions": "fixVersions"
    }
  },
  "azureDevOps": {
    "org": "https://dev.azure.com/YOUR_ORG",
    "project": "YOUR_PROJECT",
    "repoBaseUrl": "git@ssh.dev.azure.com:v3/YOUR_ORG/YOUR_PROJECT"
  },
  "services": {
    "my-service": {
      "repo": "my-service",
      "component": "Component Name",
      "componentId": "12345",
      "lead": "Team Lead Name"
    }
  },
  "slack": {
    "botToken": "xoxb-YOUR-SLACK-BOT-TOKEN",
    "userId": "UXXXXXXXXXX"
  },
  "agent": {
    "pollInterval": 300,
    "maxTicketsPerCycle": 1,
    "logDir": "./logs",
    "executionRetries": 1
  },
  "aiProvider": {
    "strategy": "single",

    "execute": {
      "provider": "claude",
      "fallbackProvider": null,
      "claude": {
        "model": "haiku",
        "maxTurns": 30,
        "timeoutMinutes": 15,
        "allowedTools": "Read,Write,Edit,Bash,Glob,Grep"
      },
      "codex": {
        "model": null,
        "timeoutMinutes": 15
      }
    },

    "debate": {
      "provider": "claude",
      "maxRounds": 3,
      "claude": {
        "model": "sonnet",
        "maxTurns": 15,
        "timeoutMinutes": 10,
        "allowedTools": "Read,Glob,Grep"
      },
      "codex": {
        "model": null,
        "timeoutMinutes": 10
      }
    },

    "evaluate": {
      "provider": "claude",
      "claude": {
        "model": "sonnet",
        "maxTurns": 5,
        "timeoutMinutes": 5,
        "allowedTools": "Read,Glob,Grep"
      }
    }
  },
  "infra": {
    "enabled": false,
    "scriptsDir": "/path/to/infra-scripts",
    "stopAfterProcessing": false
  },
  "tests": {
    "enabled": false
  }
}
